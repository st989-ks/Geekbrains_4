<resources>
    <style name="CalenderViewCustom" parent="Theme.AppCompat">
        <item name="colorAccent">@color/calenderTextColorSecond</item>
    </style>

    <style name="CalenderViewDateCustomText" parent="android:TextAppearance.DeviceDefault.Large">
        <item name="android:textColor">@color/cardview_shadow_end_color</item>
    </style>

    <style name="TextViewStart">
        <item name="android:background">#F2222121</item>
        <item name="android:textColor">@android:color/tab_indicator_text</item>
    </style>

    <style name="TextViewMiddle">
        <item name="android:background">@android:color/background_light</item>
    </style>

    <style name="LinerViewBottom">
        <item name="android:background">#1C1C1C</item>
    </style>

    <style name="ButtonFirst" parent="AppTheme">
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:textSize">@dimen/buttonSize</item>
    </style>

    <style name="AppTheme" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <item name="colorPrimary">#26282B</item>
        <item name="colorPrimaryDark">@color/outputWindow</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

</resources>